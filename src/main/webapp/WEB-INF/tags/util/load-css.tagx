<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" version="2.0">
  <jsp:output omit-xml-declaration="yes" />
  <c:set var="userLocale">
    <c:out value="${pageContext.response.locale}" default="en" />
  </c:set>

  <spring:url value="/static/css/style.min.css" var="styles_css" />
  <spring:url value="/static/js/excanvas.min.js" var="excanvas_js_url" />
  <spring:url value="/static/images/favicon.ico" var="favicon" />

  <link rel="stylesheet" type="text/css" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/themes/sunny/jquery-ui.css" />
  <link rel="stylesheet" type="text/css" href="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.0/css/jquery.dataTables.css" />
  <link rel="stylesheet" type="text/css" href="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.0/css/jquery.dataTables_themeroller.css" />
  <link rel="stylesheet" type="text/css" href="${styles_css}" />
  <link rel="SHORTCUT ICON" href="${favicon}" />
  <c:set var="userLocale">
    <c:out value="${pageContext.response.locale}" default="en" />
  </c:set>
  <script type="text/javascript" charset="utf8" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"><!-- required for FF3 and Opera --></script>
  <![CDATA[ <!--[if lte IE 8]> ]]>
  <script type="text/javascript" charset="utf8" src="${excanvas_js_url}"><!-- required for FF3 and Opera --></script>
  <![CDATA[ <![endif]--> ]]>

</jsp:root>